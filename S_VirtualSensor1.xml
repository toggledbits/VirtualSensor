<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upn-org:service-1-0">
    <specVersion>
        <major>1</major>
        <minor>0</minor>
    </specVersion>
    <serviceStateTable>
        <stateVariable sendEvents="no">
            <name>Interval</name>
            <dataType>ui4</dataType>
            <defaultValue>5</defaultValue>
            <shortCode>interval</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>NextX</name>
            <dataType>string</dataType>
            <defaultValue>0</defaultValue>
            <shortCode>nextx</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>Period</name>
            <dataType>ui4</dataType>
            <defaultValue>120</defaultValue>
            <shortCode>period</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>Amplitude</name>
            <dataType>string</dataType>
            <defaultValue>1</defaultValue>
            <shortCode>amplitude</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>Midline</name>
            <dataType>string</dataType>
            <defaultValue>0</defaultValue>
            <shortCode>midline</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>Precision</name>
            <dataType>ui4</dataType>
            <defaultValue>2</defaultValue>
            <shortCode>prec</shortCode>
        </stateVariable>
        <stateVariable sendEvents="no">
            <name>DutyCycle</name>
            <dataType>string</dataType>
            <defaultValue>50</defaultValue>
            <shortCode>duty</shortCode>
        </stateVariable>
        <stateVariable sendEvents="yes">
            <name>Enabled</name>
            <dataType>ui1</dataType>
            <defaultValue>0</defaultValue>
            <shortCode>enabled</shortCode>
        </stateVariable>
    </serviceStateTable>
    <actionList>
        <action>
            <name>SetInterval</name>
            <argumentList>
                <argument>
                    <name>newIntervalValue</name>
                    <relatedStateVariable>Interval</relatedStateVariable>
                    <direction>in</direction>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>GetInterval</name>
            <argumentList>
                <argument>
                    <name>ResultValue</name>
                    <relatedStateVariable>Interval</relatedStateVariable>
                    <direction>out</direction>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SetEnabled</name>
            <argumentList>
                <argument>
                    <name>newEnabledValue</name>
                    <direction>in</direction>
                    <relatedStateVariable>Enabled</relatedStateVariable>
                </argument>
            </argumentList>
        </action>
        <action>
            <name>SetDebug</name>
            <argumentList>
                <argument>
                    <name>debug</name>
                    <direction>in</direction>
                </argument>
            </argumentList>
        </action>
    </actionList>
</scpd>
